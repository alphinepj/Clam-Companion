    "SyncManager": "readonly",
    "BackgroundSyncManager": "readonly",
    "PeriodicSyncManager": "readonly",
    "PaymentRequest": "readonly",
    "PaymentResponse": "readonly",
    "PaymentAddress": "readonly",
    "PaymentMethodChangeEvent": "readonly",
    "PaymentRequestUpdateEvent": "readonly",
    "Credential": "readonly",
    "CredentialsContainer": "readonly",
    "FederatedCredential": "readonly",
    "PasswordCredential": "readonly",
    "PublicKeyCredential": "readonly",
    "AuthenticatorAttestationResponse": "readonly",
    "AuthenticatorAssertionResponse": "readonly",
    "AuthenticatorResponse": "readonly",
    "AuthenticatorSelectionCriteria": "readonly",
    "PublicKeyCredentialCreationOptions": "readonly",
    "PublicKeyCredentialRequestOptions": "readonly",
    "PublicKeyCredentialUserEntity": "readonly",
    "PublicKeyCredentialRpEntity": "readonly",
    "PublicKeyCredentialParameters": "readonly",
    "PublicKeyCredentialDescriptor": "readonly",
    "PublicKeyCredentialType": "readonly",
    "UserVerificationRequirement": "readonly",
